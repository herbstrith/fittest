{
  "name": "fit-integration-tests",
  "version": "1.5.0",
  "description": "FIT (Fast Integration Tests) is a tool to run integration tests fast and in parallel.",
  "main": "./core/index.js",
  "dependencies": {
    "ngrok": "^3.0.1"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^5.3.0",
    "mocha": "^5.2.0",
    "request-promise": "^4.2.2"
  },
  "scripts": {
    "lint": "eslint './core' --fix && eslint './test' --fix",
    "test:unit": "mocha './{,!(node_modules)/**}/*.spec.js'",
    "test:pass": "node ./test/pass_tests.js",
    "test:broken": "node ./test/broken_tests",
    "test": "npm run test:pass && npm run test:unit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/szanata/fit-integration-tests.git"
  },
  "author": "St√©fano Zanata <contato@szanata.com>",
  "bugs": {
    "url": "https://github.com/szanata/fit-integration-tests/issues"
  },
  "homepage": "https://github.com/szanata/fit-integration-tests#readme",
  "directories": {
    "test": "test"
  },
  "license": "ISC"
}
